# Directory navigation aliases
alias ..='cd ../'
alias ...='cd ../..'
alias ....='cd ../../..

# Grep coloring (Not as important with ag)
alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'

# Directory listing aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
